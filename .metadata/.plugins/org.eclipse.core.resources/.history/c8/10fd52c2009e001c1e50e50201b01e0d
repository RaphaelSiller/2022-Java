
public class Quadrat {
	double SeiteA;

	/** 
	 * Liefert die Länge von SeiteA
	 * @return the seiteA
	 */
	public double getSeiteA() {
		return SeiteA;
	}

	/** 
	 * Setzt die Länge von SeiteA
	 * @param seiteA - welche gesetzt werden soll
	 */
	public void setSeiteA(double seiteA) {
		SeiteA = seiteA;
	}
	
	/** 
	 * Liefert die Länge von SeiteB
	 * @return SeiteB
	 */
	public double getSeiteB() {
		return SeiteA;
	}

	/** 
	 * Setzt die Länge von SeiteB
	 * @param - welche gesetzt werden soll
	 */
	public void setSeiteB(double SeiteB) {
		this.SeiteA = SeiteB;
	}
	
	/** 
	 * Liefert den Umfang des Quadrats
	 * @return the Umfang
	 */
	public double getUmfang() {
		return SeiteA * 4;
	}

	/** 
	 * Setzt die Länge von Umfang
	 * @param - welcher gesetzt werden soll
	 */
	public void setUmfang(double Umfang) {
		this.SeiteA = Umfang * 0.25;
	}
	

}
